generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Announce {
  Id          Int       @id @default(autoincrement())
  User_Id     Int?      @map("User-Id")
  Contents    String?
  Date_Posted DateTime? @map("Date-Posted") @db.Timestamp(6)
  Pinned      Boolean   @default(false)
  User        User?     @relation(fields: [User_Id], references: [Id], onDelete: NoAction, onUpdate: NoAction, map: "announcement_User-Id_fkey")
}

model Task {
  Id      Int      @id(map: "task_pkey") @default(autoincrement())
  User_Id Int      @map("User-Id")
  Name    String
  Date    DateTime @db.Date
  User    User     @relation(fields: [User_Id], references: [Id], onDelete: NoAction, onUpdate: NoAction, map: "task_User-Id_fkey")
}

model User {
  Id          Int                  @id(map: "user_pkey") @default(autoincrement())
  Name        String               @db.VarChar(64)
  Email       String               @db.VarChar(64)
  Color       Unsupported("char")?
  Online      Boolean
  Last_online DateTime             @map("Last-online") @db.Timestamp(6)
  Announce    Announce[]
  Task        Task[]
}
